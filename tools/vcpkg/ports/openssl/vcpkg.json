{
  "name": "openssl",
  "version": "3.4.0",
  "port-version": 8,
  "description": "OpenSSL with ECH support",
  "homepage": "https://github.com/sftcd/openssl/tree/ECH-draft-13c",
  "license": "Apache-2.0",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "vcpkg-cmake-get-vars",
      "host": true
    }
  ],
  "features": {
    "fips": {
      "description": "Enable fips",
      "supports": "!static"
    },
    "tools": {
      "description": "Install openssl executable and scripts",
      "supports": "!uwp"
    }
  }
}
